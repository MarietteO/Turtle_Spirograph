import random
from turtle import Turtle, Screen

tim = Turtle()
screen = Screen()
screen.colormode(255)

tim.shape("turtle")
tim.speed(90)

for _ in range(400):
    r = random.randint(0,255)
    g = random.randint(0,255)
    b = random.randint(0,255)
    random_color = (r,g,b)
    tim.color(random_color)
    tim.circle(100)
    tim.left(5)

screen.exitonclick()
